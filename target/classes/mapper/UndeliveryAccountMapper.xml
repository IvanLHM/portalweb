<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.UndeliveryAccountMapper">
    <insert id="batchInsert">
        INSERT INTO tbl_undelivery_account (
            id, account_number, account_name, created_at, last_update
        ) VALUES 
        <foreach collection="records" item="record" separator=",">
            (
                #{record.id},
                #{record.accountNumber},
                #{record.accountName},
                CURRENT_TIMESTAMP,
                CURRENT_TIMESTAMP
            )
        </foreach>
    </insert>
    <select id="findAll" resultType="com.example.demo.entity.UndeliveryAccount">
        SELECT * FROM undelivery_account
    </select>
</mapper> 